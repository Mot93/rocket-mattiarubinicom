<!doctype html>
<html id="html" class="theme_dark" lang="{{language_code}}">



  <!-- HEAD -->
  <head>
    <title>{{title}}</title>
    <!-- META -->
    <meta charset="UTF-8">
    <meta name="author" content="Mattia Rubini">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- STYLE -->
    <link rel="stylesheet" type="text/css" href="css/common/layout.css">
    <link rel="stylesheet" type="text/css" href="css/common/header.css">
    <link rel="stylesheet" type="text/css" href="css/common/footer.css">
    <link rel="stylesheet" type="text/css" href="css/common/main.css">
    {{#each styles}}
      <link rel="stylesheet" type="text/css" href="{{this}}">
    {{/each}}
    <!-- SCRIPT -->
    <script src="js/layout.js"></script>
    {{#each scripts}}
      <script src="{{this}}"></script>
    {{/each}}
    <!-- BOOTSTRAP -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
  </head>



  <!-- BODY -->
  <body id="body" onload="enlargeMain();">
    <header id ="header">{{> common/header}}</header>
    <main id ="main">{{~> main}}</main>
    <footer id="footer">{{> common/footer}}</footer>
  </body>



</html>